#seno
def seno(x):
  """
  Função que calcula o seno do angulo x em graus

  Input
  -------
  x = float

  Output
  ------
  seno(x)
  """
  s = np.radians(x)
  return np.sin(s)
  
  #cosseno
  def cosseno(x):
  """
  Função que calcula o cosseno do angulo x em graus

  Input
  -------
  x = float

  Output
  ------
  cos(x)
  """
  c = np.radians(x)
  return np.cos(c)
